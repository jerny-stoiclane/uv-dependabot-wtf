{
  "name": "@armhr/hcm",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:sourcemap": "vite build --sourcemap",
    "build:staging": "vite build --mode staging",
    "build:analyze": "ANALYZE_BUNDLE=true vite build",
    "lint": "TIMING=1 eslint ./src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:ci": "CI=true eslint ./src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "preview": "vite preview",
    "start": "vite",
    "test:watch": "vitest",
    "test": "vitest run",
    "tsc": "tsc",
    "tsc:diagnostics": "tsc --noEmit --extendedDiagnostics",
    "tsc:analyze-deps": "tsc --noEmit --traceResolution --listFiles --listEmittedFiles --listFilesOnly",
    "tsc:trace": "tsc --generateTrace traceDir && npx @typescript/analyze-trace traceDir"
  },
  "dependencies": {
    "@armhr/ui": "*",
    "@auth0/auth0-react": "1.10.2",
    "@datadog/browser-logs": "4.46.0",
    "@datadog/browser-rum": "4.46.0",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@mui/icons-material": "5.13.7",
    "@mui/lab": "5.0.0-alpha.124",
    "@mui/material": "5.13.5",
    "@react-pdf/renderer": "3.1.9",
    "@types/react-helmet": "6.1.11",
    "axios": "1.3.4",
    "clsx": "1.2.1",
    "d3": "7.8.4",
    "d3-org-chart": "3.1.1",
    "date-fns": "2.29.3",
    "date-fns-tz": "2.0.0",
    "formik": "2.2.9",
    "formik-mui": "5.0.0-alpha.0",
    "fuse.js": "6.6.2",
    "js-cookie": "3.0.5",
    "mui-daterange-picker": "1.0.5",
    "react": "18.2.0",
    "react-avatar-editor": "13.0.2",
    "react-beautiful-dnd": "13.1.1",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-idle-timer": "5.7.2",
    "react-imask": "7.0.0",
    "react-intl": "6.4.4",
    "react-router-dom": "6.11.2",
    "remark": "15.0.1",
    "remark-html": "16.0.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@types/d3": "7.4.0",
    "@types/d3-org-chart": "3.1.2",
    "@types/js-cookie": "3.0.3",
    "@types/react-beautiful-dnd": "13.1.4"
  }
}
